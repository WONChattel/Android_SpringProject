<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.RecipeSearch">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"

        android:usesCleartextTraffic="true">
        <activity android:name="com.example.RecipeSearch.MainActivity3"></activity>
        <activity android:name="com.example.RecipeSearch.MainActivity2" />
        <activity android:name="com.example.RecipeSearch.MainSearchActivity" />
        <activity android:name="com.example.RecipeSearch.MainActivity4" />
        <activity android:name="com.example.RecipeSearch.ItemActivity" />
        <activity android:name="com.example.RecipeSearch.Menu1Activity" />
        <activity android:name="com.example.RecipeSearch.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>