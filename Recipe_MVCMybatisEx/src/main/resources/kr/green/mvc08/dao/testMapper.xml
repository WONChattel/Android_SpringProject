<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.green.mvc08.dao.TestDao">

	<select id="selectToday" resultType="string">
		select sysdate from dual
	</select>
	<select id="selectSum" resultType="int"
		parameterType="java.util.HashMap">
		select #{num1} +
		#{num2} from dual
	</select>
	<select id="selectMul" resultType="int"
		parameterType="java.util.HashMap">
		select #{num1} *
		#{num2} * #{num3} from dual
	</select>


</mapper>